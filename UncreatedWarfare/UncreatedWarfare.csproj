<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net481</TargetFramework>
    <AssemblyTitle>Uncreated Warfare</AssemblyTitle>
    <Product>Uncreated Warfare</Product>
    <Version>3.2.6</Version>
    <Authors>BlazingFlame, 420DankMeister (Daniel Willett, Ben Valkin)</Authors>
    <Company>Uncreated Network</Company>
    <Description>Main framework for the Uncreated Warfare Military Simulation server based off of the military game Squad.</Description>
    <PackageLicenseExpression>N/A</PackageLicenseExpression>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <EnablePreviewFeatures>true</EnablePreviewFeatures>
    <DoNotCopyLocalIfInGac>true</DoNotCopyLocalIfInGac>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/UncreatedStaff/UncreatedWarfare</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Plugin</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <RootNamespace>Uncreated.Warfare</RootNamespace>
    <AssemblyVersion>$(Version).0</AssemblyVersion>
    <FileVersion>$(Version).0</FileVersion>
    <BaseIntermediateOutputPath></BaseIntermediateOutputPath>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <Title>Uncreated Warfare</Title>
    <IsPublishable>False</IsPublishable>
    <NoWin32Manifest>true</NoWin32Manifest>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>portable</DebugType>
    <WarningLevel>5</WarningLevel>
    <DefineConstants>$(DefineConstants)TRACE;UNITY_ASSERTIONS</DefineConstants>
    <NoWarn>1701;1702,1591,1587,1711,1735,1573,1574,1570,1584,1658</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>portable</DebugType>
    <DefineConstants>$(DefineConstants)TRACE</DefineConstants>
    <NoWarn>1701;1702,1591,1587,1711,1735,1573,1574,1570,1584,1658</NoWarn>
  </PropertyGroup>


  <ItemGroup>
    <None Remove="Command_blank.txt" />
    <Compile Remove="Commands\Command_blank.txt" />
  </ItemGroup>

  <!-- Base U3DS installation -->
  <PropertyGroup>
    <ServerPath>C:\SteamCMD\steamapps\common\U3DS</ServerPath>
  </PropertyGroup>

  <!-- Build to module dir if it exists -->
  <PropertyGroup Condition="exists('$(ServerPath)\Modules\UncreatedWarfare\Bin')">
    <OutDir>$(ServerPath)\Modules\UncreatedWarfare\Bin</OutDir>
  </PropertyGroup>

  <!-- Prevent libraries from being copied -->
  <ItemDefinitionGroup>
    <PackageReference ExcludeAssets="runtime" />
  </ItemDefinitionGroup>

  <ItemDefinitionGroup>
    <Reference Private="false" />
  </ItemDefinitionGroup>


  <ItemGroup>

    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Net.Http" />
    <Reference Include="WindowsBase" />

    <Reference Include="System.Diagnostics.DiagnosticSource">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\System.Diagnostics.DiagnosticSource.dll</HintPath>
    </Reference>

    <Reference Include="Assembly-CSharp">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\Assembly-CSharp.dll</HintPath>
    </Reference>

    <Reference Include="com.rlabrecque.steamworks.net">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\com.rlabrecque.steamworks.net.dll</HintPath>
    </Reference>

    <Reference Include="Newtonsoft.Json">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\Newtonsoft.Json.dll</HintPath>
    </Reference>

    <Reference Include="SDG.NetPak.Runtime">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\SDG.NetPak.Runtime.dll</HintPath>
    </Reference>

    <Reference Include="SDG.NetTransport">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\SDG.NetTransport.dll</HintPath>
    </Reference>

    <Reference Include="StackCleaner">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\StackCleaner.dll</HintPath>
    </Reference>

    <Reference Include="Stripe.net">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\Stripe.net.dll</HintPath>
    </Reference>

    <Reference Include="Uncreated.UI">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\Uncreated.UI.dll</HintPath>
    </Reference>

    <Reference Include="UncreatedNetworking">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\UncreatedNetworking.dll</HintPath>
    </Reference>

    <Reference Include="UniTask">
      <HintPath>$(ServerPath)\Modules\UncreatedWarfare\Libraries\UniTask.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine.AssetBundleModule">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.AssetBundleModule.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine.CoreModule">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.CoreModule.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine.ImageConversionModule">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.ImageConversionModule.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine.PhysicsModule">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.PhysicsModule.dll</HintPath>
    </Reference>

    <Reference Include="UnityEngine.UnityWebRequestModule">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnityEngine.UnityWebRequestModule.dll</HintPath>
    </Reference>

    <Reference Include="UnturnedDat">
      <HintPath>$(ServerPath)\Unturned_Data\Managed\UnturnedDat.dll</HintPath>
    </Reference>

  </ItemGroup>


  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="Resources\icon.png">
      <Pack>True</Pack>
      <PackagePath />
    </None>
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Lib.Harmony" Version="2.2.2" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
    <PackageReference Include="MySqlConnector" Version="2.2.7" />
    <PackageReference Include="System.Text.Json" Version="7.0.0" />
  </ItemGroup>
</Project>